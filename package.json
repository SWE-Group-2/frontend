{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "lint:style": "prettier --write src/**/*.{css,vue}",
    "format": "prettier .  --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.23.0",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "vue": "^3.4.15",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/test-utils": "^2.4.5",
    "@vue/vue3-jest": "^29.2.6",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "vite": "^5.0.11"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "npm run lint"
    ],
    "*.{css,vue}": [
      "npm run lint:style"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "@vue/vue3-jest"
    }
  }
}
